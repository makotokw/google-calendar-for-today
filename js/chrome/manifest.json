{
  "name": "Google Calendar for Today",
  "version": "@VERSION",
  "description": "Displays events in your Google Calendar via day.",
  "default_locale": "en",
  "icons": {
      "128": "icon_128.png"
  },
  "background_page": "background.html",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Google Calendar for Today",
    "popup": "popup.html"
  },
  "permissions": [ "tabs", "http://*.google.com/", "https://*.google.com/"]
}